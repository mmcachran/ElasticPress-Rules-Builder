{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "yoda": [
            2
        ],
        "indent": [
            2,
            "tab",
            {
                "SwitchCase": 2
            }
        ],
        "quotes": [
            2,
            "single"
        ],
        "semi": [
            2,
            "always"
        ],
        "space-in-parens": [
            0
        ],
        "no-console": [
            0
        ],
        "no-alert": [
            1
        ],
        "camelcase": [
            0
        ]
    }
}